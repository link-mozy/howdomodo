    "    for div_theater_time in div_theater_time_list:\n",
    "        \n",
    "        # screen, floor, seat_total get\n",
    "        screen, floor, seat_total = [div_theater_time.text for div_theater_time in div_theater_time.find('span', attrs={\"class\":\"title\"}).find_all('span')]\n",
    "        li_theater_time_list = [_ for _ in div_theater_time.find('ul').find_all('li') if _.has_attr('data-index')]\n",
    "\n",
    "        if len(li_theater_time_list) > 1:\n",
    "\n",
    "            for li_theater_time in li_theater_time_list:\n",
    "                theater_info = {} # 리턴에 담을 딕셔너리\n",
    "                start_time, seat_count, end_time, sreader_mod = [_.text for _ in li_theater_time.find_all(['span','div']) if _.has_attr('class')]\n",
    "                theater_info[\"screen\"] = screen\n",
    "                theater_info[\"floor\"] = floor\n",
    "                theater_info[\"seat_total\"] = seat_total\n",
    "                theater_info[\"start_time\"] = start_time\n",
