   "source": [
    "# 지역, 극장명 목록 구하기 (롯데)\n",
    "def generateAreaTheaterToDict(li_depth1_list):\n",
    "    area_theater_dict = {} # 리턴 딕셔너리\n",
    "\n",
    "    for idx, li_depth1 in enumerate(li_depth1_list):\n",
    "        area = li_depth1.find(\"a\").text\n",
    "        area = area.split(\"(\")[0].split(\"/\") # area : 지역명\n",
    "        theater_name_list = [_.text for _ in li_depth1.find(\"ul\").find_all(\"li\") if _.has_attr(\"class\")] # 극장명 리스트\n",
    "        areaIdx_theaters_dict = {} # 리턴에 담을 딕셔너리\n",
    "        areaIdx_theaters_dict[\"area_idx\"] = idx + 1\n",
    "\n",
    "        if len(area) > 1:\n",
    "            for i in range(len(area)):\n",
    "                areaIdx_theaters_dict[\"theaters\"] = theater_name_list\n",
    "                area_theater_dict[area[i]] = areaIdx_theaters_dict\n",
    "\n",
    "        else :\n",
    "            area = area[0]\n",
    "            areaIdx_theaters_dict[\"theaters\"] = theater_name_list\n",
    "            area_theater_dict[area] = areaIdx_theaters_dict \n",
    "\n",
    "    return area_theater_dict"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getAreaIdxAndTheaterIdx(area_theater_dict, user_area, user_theater_name):\n",
    "    return area_theater_dict[user_area]['area_idx'], area_theater_dict[user_area]['theaters'].index(user_theater_name)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def generateLotteTheaterInfo(html):\n",
    "    lotte_theater_info = [] # 리턴 리스트\n",
    "    soup = BeautifulSoup(html, \"lxml\")\n",
    "    body = soup.body\n",
    "\n",
    "    div_time_select_list = body.find(\"div\", attrs={\"id\":\"mCSB_65\"}).find_all(\"div\", attrs={\"class\":\"group_time_select\"})\n",
    "\n",
    "    for div_time_select in div_time_select_list:\n",
    "        div_movie_info = div_time_select.find_all(\"div\")[0]\n",
    "        div_theater_info = div_time_select.find_all(\"div\")[1]\n",
    "        \n",
    "        # 등급, 영화 제목 수집    \n",
    "        grade = div_movie_info.find(\"span\").text\n",
    "        title = div_movie_info.find(\"strong\").text\n",
    "        # 스크린\n",
    "        screen = \",\".join([_.text for _ in div_theater_info.find_all(\"ul\")[0].find_all(\"li\")])\n",
    "        # 영화 시간, 좌석, 상영관\n",
    "        li_theater_info_list = div_theater_info.find_all(\"ul\")[1]\n",
    "        \n",
    "        for li_theater_info in li_theater_info_list:\n",
    "            theater_info = {} # 리턴에 넣을 딕셔너리\n",
    "            \n",
    "            dd_theater_info = [_ for _ in li_theater_info.find_all(\"dd\")]\n",
    "            start_time = dd_theater_info[0].find(\"strong\").text\n",
    "            end_time = dd_theater_info[0].find(\"div\").text\n",
    "            seat_split = dd_theater_info[1].text.replace(\" \", \"\").split(\"/\")\n",
    "            \n",
    "            if len(seat_split) > 1:\n",
    "                seat_count, seat_total = seat_split\n",
    "                \n",
    "            else :\n",
    "                seat_count, seat_total = seat_split[0], seat_split[0]\n",
    "            hall = dd_theater_info[2].text\n",
    "            \n",
    "            theater_info[\"grade\"] = grade\n",
    "            theater_info[\"title\"] = title\n",
    "            theater_info[\"screen\"] = screen\n",
    "            theater_info[\"start_time\"] = start_time\n",
    "            theater_info[\"end_time\"] = end_time\n",
    "            theater_info[\"seat_count\"] = seat_count\n",
    "            theater_info[\"seat_total\"] = seat_total\n",
    "            theater_info[\"hall\"] = hall\n",
    "            lotte_theater_info.append(theater_info)\n",
    "            \n",
    "    return lotte_theater_info"
   ]
  },
