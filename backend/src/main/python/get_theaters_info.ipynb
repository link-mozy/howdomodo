    "# 지역, 극장 dict형으로 만들기\n",
    "def generateAreaTheaterToDict(theater_list):\n",
    "    area_theater_dict = {} # 리턴 딕셔너리\n",
    "\n",
    "    for idx, li_theater in enumerate(theater_list) :\n",
    "        area = li_theater.find(\"a\").text.split(\"(\")[0].strip() # area : 지역명\n",
    "        li_theaters_list = [li.find(\"a\").text for li in li_theater.find(\"div\").find_all(\"li\")] # 극장명 리스트\n",
    "        \n",
    "        if area != \"\" :\n",
    "            area = area.split(\"/\") # 지역명 정제\n",
    "            areaIdx_theaters_dict = {} # 리턴에 담을 딕셔너리\n",
    "            areaIdx_theaters_dict[\"area_idx\"] = idx\n",
    "            \n",
    "            if len(area) > 1:\n",
    "                for i in range(0, len(area)):\n",
    "                    areaIdx_theaters_dict[\"theaters\"] = li_theaters_list\n",
    "                    area_theater_dict[area[i]] = areaIdx_theaters_dict\n",
    "\n",
    "            else :\n",
    "                area = area[0]\n",
    "                areaIdx_theaters_dict[\"theaters\"] = li_theaters_list\n",
    "                area_theater_dict[area] = areaIdx_theaters_dict    \n",
    "                \n",
    "    return area_theater_dict"
   ]
  },
