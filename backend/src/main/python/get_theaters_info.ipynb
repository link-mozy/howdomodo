    "# 위도,경도 구하기\n",
    "def getLatLon(address, reg):\n",
    "    try:\n",
    "        url = \"https://www.google.co.kr/maps/place/\" + address\n",
    "        print(url)\n",
    "        headers = {\"user-agent\":\"Mozilla/5.0 (Macintosh; Intel Mac OS X 10_14_4) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/75.0.3770.100 Safari/537.36\"}\n",
    "        resp = requests.request('get', url)\n",
    "        dom = BeautifulSoup(resp.text, 'html5lib')\n",
    "        meta_list = dom.find('head').select('meta')\n",
    "\n",
    "        lat_lon = []\n",
    "        for content in meta_str_list:\n",
    "            m = reg.match(content)\n",
    "            if m:\n",
    "                lat_lon.append(m.group(2))\n",
    "\n",
    "        lat_lon = lat_lon[0].split('%2C')\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(e)\n",
    "        lat_lon = ['', '']\n",
    "        \n",
    "    return lat_lon"
   ]
