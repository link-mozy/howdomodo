    "def getLatLon(addresses):\n",
    "    result = {}\n",
    "    if len(addresses) == 1 :\n",
    "        # 바로 저장\n",
    "        address_ = addresses[0]\n",
    "        jibunAddress = address_['jibunAddress']\n",
    "        # 위도(latitude : y) 경도(longitude : x)\n",
    "        lat = address_[\"y\"]\n",
    "        lon = address_[\"x\"]\n",
    "\n",
    "        result['jibunAddress'] = jibunAddress\n",
    "        result['lat'] = lat\n",
    "        result['lon'] = lon\n",
    "    \n",
    "    elif len(addresses) > 1:\n",
    "        # 맞는거 찾아서 저장\n",
    "        point_list = []\n",
    "\n",
    "        for address_ in addresses:\n",
    "            addressElements = address_['addressElements']\n",
    "            point = 0\n",
    "\n",
    "            for addressElement in addressElements:\n",
    "                longName = addressElement['longName']\n",
    "                shortName = addressElement['shortName']\n",
    "                if any(elem in address_split for elem in [longName, shortName]):\n",
    "                    point += 1\n",
    "\n",
    "            point_list.append(point)\n",
    "\n",
    "        point_max_index = point_list.index(max(point_list))\n",
    "        address_ = addresses[point_max_index]\n",
    "        jibunAddress = address_['jibunAddress']\n",
    "        # 위도(latitude : y) 경도(longitude : x)\n",
    "        lat = address_[\"y\"]\n",
    "        lon = address_[\"x\"]\n",
    "\n",
    "        result['jibunAddress'] = jibunAddress\n",
    "        result['lat'] = lat\n",
    "        result['lon'] = lon\n",
    "        \n",
    "    return result"
   ]
